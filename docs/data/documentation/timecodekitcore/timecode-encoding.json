."}]},{"code":["struct TimecodeListView: View {","    @State var model: [Timecode]","    @State var selection: Timecode","    ","    var body: some View {","        List(selection: $selection) {","            ForEach(model) { item in","                Text(item.stringValue()).tag(item)","            }","            .dropDestination(for: Timecode.self) { items, location in","                add(items: items)","                return true","            }","        }","        .copyable([selection])","        .pasteDestination(for: Timecode.self) { items in","            add(items: items)","        }","    }","    ","    func add(items: [Timecode]) {","        for item in items {","            \/\/ Here is where you can validate pasted timecode before accepting it.","            \/\/ See the `validate()` method inline help or Encoding section in the ","            \/\/ documentation for details. ","            let validatedTimecode = TimecodeField.validate(pastedTimecode: item, ... )","","            \/\/ Timecode's default `Identifiable` implementation uses self","            \/\/ which means we cannot have two identical timecodes in a SwiftUI array","            if !model.contains(validatedTimecode) { model.append(validatedTimecode) }","        }","    }","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"text":"See the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/TimecodeKitCore\/documentation\/TimecodeKitCore\/Timecode-Encoding#Pasted-Timecode-Validation-Against-Local-Context"},{"type":"text","text":" section on how to validate pasted timecode."}]},{"style":"important","type":"aside","content":[{"inlineContent":[{"text":"","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"If SwiftUI view modifiers using the ","type":"text"},{"isActive":true,"identifier":"https:\/\/developer.apple.com\/documentation\/coretransferable\/transferable","overridingTitle":"Transferable","type":"reference","overridingTitleInlineContent":[{"type":"codeVoice","code":"Transferable"}]},{"text":" protocol are invoked without exporting ","type":"text"},{"code":"Timecode","type":"codeVoice"},{"type":"text","text":"’s UT Type, this error will be thrown:"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"code":"Type \"com.orchetect.TimecodeKit.timecode\" was expected to be declared and exported in the Info.plist of MyApp.app, but it was not found.","type":"codeVoice"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"See the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/TimecodeKitCore\/documentation\/TimecodeKitCore\/Timecode-Encoding#UT-Types"},{"text":" section for information on how to export this type in your app.","type":"text"}]}],"name":"Important"},{"text":"Pasted Timecode Validation Against Local Context","level":2,"anchor":"Pasted-Timecode-Validation-Against-Local-Context","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"When accepting timecode pasted from the clipboard, it is common to validate it against a local context."}]},{"inlineContent":[{"text":"For example, you may want to constrain pasted timecode to a certain frame rate, subframes base and upper limit.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"code":"TimecodeKitUI","type":"codeVoice"},{"type":"text","text":" offers static API to perform this validation by supplying policies to validate against."}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["@TimecodeState private var timecode: Timecode","","\/\/ pass in the `Timecode` instance received from the pasteboard","\/\/ from the `pasteDestination()` or `onPasteCommand()` view modifiers:","func validate(pastedTimecode: Timecode) {","    guard let newTimecode = TimecodeField.validate(","        pastedTimecode: pastedTimecode,","        localTimecodeProperties: timecode.properties,","        pastePolicy: .preserveLocalProperties,","        validationPolicy: .enforceValid